<view class="content pb-30"><view class="input"><text>联系人</text><input type="text" value="{{userName}}" placeholder="请输入联系人姓名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" onInput="__e"/></view><view class="input"><text>性别</text><view class="sex" a:for="{{sexList}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item})}}" class="{{(('item')+' '+((gender==item.id)?'cur':''))}}" onTap="__e">{{item.name}}</view></view></view><view class="input"><text>手机号码</text><input type="text" value="{{userPhone}}" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','userPhone','$event',[]]]]]]}}" onInput="__e"/></view><view data-event-opts="{{[['tap',[['toggleCommunitySelection',['$event']]]]]}}" class="input" onTap="__e"><text>社区信息</text><input placeholder="请输入社区" data-event-opts="{{[['input',[['__set_model',['','areaName','$event',[]]],['searchCommunity',['$event']]]]]}}" value="{{areaName}}" onInput="__e"/><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="icon" onTap="__e"></view></view><block a:if="{{isCommunitySelectionOpen}}"><view class="community-options"><view data-event-opts="{{[['tap',[['selectCommunity',['$0'],[[['communityOptions','id',option.id]]]]]]]}}" class="option-item" onTap="__e" a:for="{{communityOptions}}" a:for-item="option" a:for-index="__i1__" a:key="id">{{''+option.community+''}}</view></view></block><view class="input"><text>详细地址</text><input type="text" value="{{address}}" placeholder="例:xx号xx楼xx门牌" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" onInput="__e"/></view><block a:if="{{id}}"><view data-event-opts="{{[['tap',[['edit',['$event']]]]]}}" class="btn" onTap="__e">确认修改</view></block><block a:else><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn" onTap="__e">保存地址</view></block></view>
<view><view class="tab"><view data-event-opts="{{[['tap',[['selectTab',['$0'],[[['tabs','id',item.id,'id']]]]]]]}}" class="{{(('item')+' '+((tabCur==item.id)?'cur':''))}}" onTap="__e" a:for="{{tabs}}" a:for-item="item" a:for-index="__i0__" a:key="id">{{''+item.name+''}}</view></view><view class="card-list"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['list','id',item.id,'id']]]]]]]}}" class="card" onTap="__e" a:for="{{list}}" a:for-item="item" a:for-index="__i1__" a:key="id"><view class="{{(((('header')+' '+'flex-between')+' '+'size-28')+' '+('status-bg-'+item.status))}}"><view class="white">{{"下单时间："+item.create_time}}</view><block a:if="{{item.status==1||item.status==6}}"><view class="white">服务中</view></block><block a:if="{{item.status==3}}"><view class="white">已完成</view></block></view><view class="body px-26"><view class="pt-36">{{'订单号：'+item.ordernum+''}}</view></view><view class="p-20 mt-6"><view class="size-32">{{item.address}}</view><view class="size-24 gray-9 mt-2">{{"距你"+item.distance+"米"}}</view></view></view><block a:if="{{list.length==0}}"><view class="empty"><image src="../../static/empty.png" mode></image><view>暂无相关订单～～</view></view></block></view><order-status vue-id="e3b01f94-1" show="{{show}}" data-event-opts="{{[['^closePopup',[['closePopup']]]]}}" onClosePopup="__e" onVueInit="__l"></order-status></view>
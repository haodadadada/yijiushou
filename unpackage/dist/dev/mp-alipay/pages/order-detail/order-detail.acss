.container {
  padding: 22rpx 14rpx 100rpx;
}
.container .icon {
  width: 100rpx;
  height: 100rpx;
}
.container .badge {
  font-size: 24rpx;
  font-weight: 400;
  color: #0f7b32;
  line-height: 44rpx;
  background: #ebfff1;
  border-radius: 22rpx;
  padding: 0 20rpx;
}
.container .icon-1 {
  min-width: 30rpx;
  width: 30rpx;
  height: 36rpx;
  margin-right: 20rpx;
}
.container .avatar {
  width: 68rpx;
  height: 68rpx;
  border-radius: 100%;
}
.fixed-btn {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  height: 98rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.btn {
  width: 200rpx;
  border-radius: 34rpx;
  line-height: 68rpx;
  box-shadow: none;
  margin-right: 30rpx;
}
.cancel-btn {
  background: #fff;
  border: 1px solid #aaaaaa;
  color: #aaaaaa;
}
.chat-btn {
  background: #0b8f3a;
  color: #ffffff;
}
.popup {
  padding: 0 30rpx;
}
.popup .header {
  padding: 32rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: #222222;
  border-bottom: 1rpx solid #efefef;
}
.popup .body .u-input {
  background: #f5f5f5;
  border: 0;
}
.popup .body .list {
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.popup .body .list .item {
  padding: 0 40rpx;
  font-size: 28rpx;
  color: #606060;
  line-height: 66rpx;
  background: #ffffff;
  border-radius: 66rpx;
  border: 1rpx solid #e2e2e2;
  margin-bottom: 30rpx;
  margin-right: 20rpx;
}
.popup .footer {
  text-align: center;
  display: flex;
  justify-content: flex-end;
  padding-bottom: 28rpx;
}
.popup .footer .btn-1 {
  font-size: 30rpx;
  color: #ffffff;
  line-height: 74rpx;
  width: 198rpx;
  background: #0b8f3a;
  border-radius: 12rpx;
}
.popup .footer .btn-2 {
  font-size: 30rpx;
  color: #222222;
  line-height: 74rpx;
  width: 198rpx;
  background: #dadada;
  border-radius: 12rpx;
  margin-left: 38rpx;
}
.success-popup {
  width: 604rpx;
  height: 558rpx;
  position: relative;
}
.success-popup image {
  width: 562rpx;
  height: 450rpx;
}
.success-popup .text {
  text-align: center;
}
.success-popup .text-1 {
  font-size: 30rpx;
  font-weight: 400;
  color: #fff;
}
.success-popup .text .btn {
  text-align: center;
  width: 316rpx;
  height: 88rpx;
  background: #ffcfbb;
  border-radius: 88rpx;
  border: 2rpx solid #ffcfbb;
  padding: 6rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 60rpx auto 0;
}
.success-popup .text .btn view {
  color: #fff;
  background: #e2581c;
  width: 100%;
  height: 100%;
  border-radius: 88rpx;
  line-height: 88rpx;
}

